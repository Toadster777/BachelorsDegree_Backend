{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "subcategorySpecificAttributes": {
      "type": "dynamiczone",
      "components": [
        "attribute-sets.laptop-attributes",
        "attribute-sets.pc-components",
        "attribute-sets.mobile_phones",
        "attribute-sets.pc_peripherals",
        "attribute-sets.photo-cameras",
        "attribute-sets.tablets",
        "attribute-sets.tv"
      ]
    },
    "manufacturer": {
      "type": "string",
      "required": true,
      "maxLength": 35
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategory.subcategory",
      "inversedBy": "products"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "string",
      "maxLength": 35,
      "required": true
    },
    "carouselMedia": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "cardPhoto": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
